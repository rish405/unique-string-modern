{
  "name": "unique-string-modern",
  "version": "0.1.0",
  "description": "Cryptographically strong, URL-safe unique string shorter and faster than uuid",
  "keywords": [
    "unique",
    "string",
    "crypto",
    "base64url",
    "uuid",
    "id"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify --clean --treeshake --out-dir dist",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://example.com/your-repo.git"
  },
  "author": "",
  "license": "MIT"
}

